 {\n      const { data } = await addProfile({\n        variables: { name },\n      });\n\n      setName('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Add yourself to the list...</h3>\n      <form\n        className=\"flex-row justify-center justify-space-between-md align-center\"\n        onSubmit={handleFormSubmit}\n      >\n        <div className=\"col-12 col-lg-9\">\n          <input\n            placeholder=\"Add your profile name...\"\n            value={name}\n            className=\"form-input w-100\"\n            onChange={(event) => setName(event.target.value)}\n          />\n        </div>\n\n        <div className=\"col-12 col-lg-3\">\n          <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n            Add Profile\n          </button>\n        </div>\n        {error && (\n          <div className=\"col-12 my-3 bg-danger text-white p-3\">\n            Something went wrong...\n          </div>\n        )}\n      </form>\n    </div>\n  );\n};\n\nexport default ProfileForm;\n","/Users/jeromechenette/Desktop/today/01/19-Ins_React-Router/client/src/components/SkillsList/index.js",[],"/Users/jeromechenette/Desktop/today/01/19-Ins_React-Router/client/src/components/SkillForm/index.js",["60"],"import React, { useState } from 'react';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_SKILL } from '../../utils/mutations';\n\nconst SkillForm = ({ profileId }) => {\n  const [skill, setSkill] = useState('');\n\n  const [addSkill, { error }] = useMutation(ADD_SKILL);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addSkill({\n        variables: { profileId, skill },\n      });\n\n      setSkill('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h4>Endorse some more skills below.</h4>\n      <form\n        className=\"flex-row justify-center justify-space-between-md align-center\"\n        onSubmit={handleFormSubmit}\n      >\n        <div className=\"col-12 col-lg-9\">\n          <input\n            placeholder=\"Endorse some skills...\"\n            value={skill}\n            className=\"form-input w-100\"\n            onChange={(event) => setSkill(event.target.value)}\n          />\n        </div>\n\n        <div className=\"col-12 col-lg-3\">\n          <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n            Endorse Skill\n          </button>\n        </div>\n        {error && (\n          <div className=\"col-12 my-3 bg-danger text-white p-3\">\n            Something went wrong...\n          </div>\n        )}\n      </form>\n    </div>\n  );\n};\n\nexport default SkillForm;\n","/Users/jeromechenette/Desktop/today/01/19-Ins_React-Router/client/src/utils/mutations.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":29,"column":15,"nodeType":"67","messageId":"68","endLine":29,"endColumn":19},{"ruleId":"65","severity":1,"message":"66","line":15,"column":15,"nodeType":"67","messageId":"68","endLine":15,"endColumn":19},"no-native-reassign",["69"],"no-negated-in-lhs",["70"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]